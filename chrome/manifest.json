{
  "manifest_version": 2,

  "name": "Vox Charta",
  "description": "This extensions adds Vox Charta links to arXiv abstracts",
  "version": "1.0",

  "content_scripts": [
  {
    "matches": [
      "http://arxiv.org/abs/*"
    ],
    "js": ["jquery-2.1.4.min.js", "popup.js"]
  }
  ],

  "permissions": [
    "tabs",
    "http://*/*", "https://*/*"
  ]
}