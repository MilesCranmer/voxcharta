{
  "manifest_version": 2,

  "name": "Vox Charta Chrome Plugin",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "0.1",

  "browser_action": {
    "default_icon": "yellowstar.png",
    "default_popup": "popup.html",
    "default_title": "+1"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://voxcharta.org/"
  ]
}